		"prepack": "yarn build"
	},
	"files": [
		"dist/source"
	],
	"keywords": [
		"config",
		"store",
		"app",
		"storage",
		"conf",
		"configuration",
		"settings",
		"preferences",
		"json",
		"data",
		"persist",
		"persistent",
		"save",
		"load",
		"read",
		"write",
		"cache"
	],
	"dependencies": {
		"ajv": "^8.17.1",
		"ajv-formats": "^3.0.1",
		"atomically": "^2.0.3",
		"debounce-fn": "^6.0.0",
		"dot-prop": "^9.0.0",
		"env-paths": "^3.0.0",
		"json-schema-typed": "^8.0.1",
		"semver": "^7.6.3",
		"uint8array-extras": "^1.4.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^8.0.1",
		"@types/node": "^24.3.0",
		"@types/semver": "^7.5.8",
		"ava": "^6.1.3",
		"del": "^8.0.0",
		"delay": "^6.0.0",
		"p-event": "^6.0.1",
		"rimraf": "^6.0.1",
		"tempy": "^3.1.0",
		"ts-node": "^10.9.2",
		"tsd": "^0.33.0",
		"typescript": "^5.6.3",
		"xo": "^1.2.2"
	},
	"xo": {
		"rules": {
			"@typescript-eslint/ban-ts-comment": "off",
			"unicorn/prevent-abbreviations": "off"
		}
	}
